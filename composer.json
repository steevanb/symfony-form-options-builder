{
    "name": "steevanb/symfony-form-options-builder",
    "license": "Apache-2.0",
    "type": "library",
    "description": "buildForm() with objects instead of array",
    "autoload": {
        "psr-4": {
            "Steevanb\\SymfonyFormOptionsBuilder\\": "src/"
        }
    },
    "require": {
        "php": "^7.1||^8.0",
        "symfony/form": "^3.0||^4.0||^5.0||^6.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.8",
        "symfony/doctrine-bridge": "^6.3",
        "symfony/security-csrf": "^6.3"
    },
    "scripts": {
        "post-install-cmd": [
            "@rm-composer-lock"
        ],
        "post-update-cmd": [
            "@rm-composer-lock"
        ],
        "rm-composer-lock": "rm composer.lock"
    }
}
